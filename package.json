{
  "name": "taki",
  "version": "1.3.1",
  "description": "Take a snapshot of any website.",
  "repository": {
    "url": "egoist/taki",
    "type": "git"
  },
  "bin": "dist/cli.js",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "snapshot",
    "prerender",
    "crawler"
  ],
  "scripts": {
    "test": "ava",
    "prepublishOnly": "npm run build",
    "build": "rollup -c",
    "watch": "rollup -c --watch"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "ava": {
    "files": ["test/**/*.test.ts"],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  },
  "dependencies": {
    "cac": "^6.5.8",
    "debug": "^4.1.1",
    "html-minifier": "^4.0.0",
    "puppeteer-core": "^3.0.2",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/html-minifier": "^3.5.3",
    "@types/puppeteer-core": "^2.0.0",
    "ava": "^3.8.1",
    "get-port": "^5.1.1",
    "prettier": "^2.0.5",
    "rollup": "^2.7.6",
    "rollup-plugin-typescript2": "^0.27.0",
    "sirv": "^0.4.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  }
}
