{
  "name": "taki",
  "version": "1.3.1",
  "description": "Take a snapshot of any website.",
  "repository": {
    "url": "egoist/taki",
    "type": "git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "snapshot",
    "prerender",
    "crawler"
  ],
  "scripts": {
    "test": "ava",
    "prepublishOnly": "npm run build",
    "build": "rm -rf dist && tsup src/index.ts --dts"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "sucrase/register"
    ]
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "debug": "4.1.1",
    "html-minifier": "4.0.0",
    "puppeteer-core": "7.0.4"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/html-minifier": "3.5.3",
    "@types/puppeteer-core": "5.4.0",
    "ava": "3.11.0",
    "get-port": "5.1.1",
    "prettier": "2.0.5",
    "sirv": "1.0.1",
    "tsup": "^3.12.1",
    "typescript": "4.1.4"
  }
}
